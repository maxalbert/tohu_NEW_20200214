[run]
branch = True

[report]
omit =
    */python?.?/*
    */tests/*
    */_version.py
    tohu/versioneer.py
    tohu/logging.py
    *__init__*

exclude_lines =
    # Need to explicitly re-enable the standard pragma to be able to use it.
    pragma: no cover

    if __name__ == '__main__':
    def __repr__

show_missing = True
